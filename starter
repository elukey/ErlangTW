#!/usr/bin/env escript
%% -*- erlang -*-
%%! erl -pa ebin/
main([ConfigFilePath]) ->
    try
        runner:main(ConfigFilePath,sequential)
    catch
    	TypeOfError:ErrorValue ->  io:format("\Exception ~w ~w, stacktrace: ~w \n",[TypeOfError, ErrorValue, erlang:get_stacktrace()])	
    end;

main(_) ->
    try
    	ConfigFilePath = ".config",
        runner:main(ConfigFilePath,sequential)
    catch
    	TypeOfError:ErrorValue ->  io:format("\Exception ~w ~w, stacktrace: ~w \n",[TypeOfError, ErrorValue, erlang:get_stacktrace()])	
    end.


